{
  "name": "test-job",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./build/index.js",
    "build": "tsc",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect=9930 --respawn --transpile-only -- ./src/index.ts",
    "test": "jest",
    "lint": "eslint ./src --max-warnings 0",
    "format": "prettier ./src --list-different",
    "typecheck": "tsc --project tsconfig.json",
    "migration:create": "../../node_modules/typeorm/cli.js migration:create",
    "migration:generate": "ts-node -r tsconfig-paths/register --project ./tsconfig.json --transpile-only ../../node_modules/typeorm/cli.js migration:generate -d src/migrations",
    "migration:show": "ts-node -r tsconfig-paths/register --transpile-only ../../node_modules/typeorm/cli.js migration:show",
    "migration:revert": "ts-node -r tsconfig-paths/register --transpile-only ../../node_modules/typeorm/cli.js migration:revert",
    "migration:run": "ts-node --project ./tsconfig.json --transpile-only ../../node_modules/typeorm/cli.js migration:run",
    "migration:run-docker": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run"
  },
  "author": "evgenytk",
  "license": "ISC",
  "dependencies": {
    "@types/got": "9.6.12",
    "body-parser": "1.20.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.18.2",
    "got": "11.8.3",
    "openai": "4.11.1",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "routing-controllers": "0.9.0",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "4.2.0",
    "typedi": "0.8.0",
    "typeorm": "0.2.45",
    "typeorm-seeding": "^1.6.1",
    "typeorm-typedi-extensions": "0.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.14",
    "@types/glob": "7.1.4",
    "@types/jest": "^29.5.5",
    "@types/multer": "1.4.7",
    "@types/node": "16.7.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.3"
  }
}
